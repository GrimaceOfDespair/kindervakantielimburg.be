<?xml version="1.0"?>
<configuration>
	<system.web>
    <pages enableViewState="true" validateRequest="false" theme="" styleSheetTheme="">
			<controls>
				<add tagPrefix="n2" assembly="N2" namespace="N2.Web.UI.WebControls" />
			</controls>
			<namespaces>
				<add namespace="N2.Templates.Mvc.Areas.Management.Models" />
			</namespaces>
		</pages>

		<authorization>
			<allow users="Admin" roles="Administrators,Editors,Writers" />
			<deny users="*" />
		</authorization>
	</system.web>
</configuration>